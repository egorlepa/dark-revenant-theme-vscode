{
	"$schema": "vscode://schemas/color-theme",
	"name": "Dark Revenant",
	"colors": {
		"focusBorder": "#00000000",
		"badge.background": "#028d00",
		"activityBarBadge.background": "#028d00",
		"button.background": "#028d00",
		"button.hoverBackground": "#03ab00",
		"widget.border": "#25ff21b5",
		"widget.shadow": "#25ff2158",
		"gitDecoration.addedResourceForeground": "#97ff56",
		"gitDecoration.modifiedResourceForeground": "#80f0ff",
		"gitDecoration.stageModifiedResourceForeground": "#80f0ff",
		"gitDecoration.renamedResourceForeground": "#80f0ff",
		"gitDecoration.deletedResourceForeground": "#ff6b6b",
		"gitDecoration.stageDeletedResourceForeground": "#ff6b6b",
		"gitDecoration.untrackedResourceForeground": "#ff6b6b",
		"textLink.foreground": "#61afe1",
		"errorForeground": "#ff5c7c",
		"editorCursor.foreground": "#F8F8F2",
		"editor.background": "#1E1E1E",
		"sideBar.background": "#1E1E1E",
		"activityBar.background": "#1E1E1E",
		"titleBar.activeBackground": "#1E1E1E",
		"titleBar.inactiveBackground": "#1E1E1E",
		"statusBar.background": "#1E1E1E",
		"menu.background": "#1E1E1E",
		"peekView.border": "#25ff21b5",
		"editorGroup.border": "#646464",
		"tab.activeBackground": "#1E1E1E",
		"tab.selectedBackground": "#1E1E1E",
		"tab.activeBorder": "#25ff21b5",
		"panel.border": "#646464",
		"panelTitle.activeBorder": "#25ff21b5",
		"peekViewEditor.background": "#252525",
		"sideBar.border": "#646464",
		"statusBar.focusBorder": "#00000000",
		"editor.lineHighlightBackground": "#2a2a2a",
		"editor.lineHighlightBorder": "#00000000",
	},
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"keyword": "#c88cdc",
		"namespace": "#96be78",
		"variable.defaultLibrary": "#FFC86E",
		"parameter": "#BBBBBB",
		"type": "#71bbd2",
		"type.defaultLibrary": "#c88cdc",
		"annotation": "#FFC86E",
		"annotationMember": "#BBBBBB",
		"interface": "#71bbd2",
		"class": "#71bbd2",
		"enum": "#71bbd2",
		"property": "#BBBBBB",
	},
	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"scope": [
				"keyword",
				"keyword.operator.new",
				"storage",
				"entity.name.type.any",
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#c88cdc"
			}
		},
		{
			"scope": [
				"entity.name.type",
				"storage.type.java",
				"storage.type.generic.java",
			],
			"settings": {
				"foreground": "#71bbd2"
			}
		},
		{
			"scope": [
				"constant.language",
				"entity.name.function",
				"storage.type.annotation"
			],
			"settings": {
				"foreground": "#ffc86e"
			}
		},
		{
			"scope": [
				"string",
				"number",
				"constant.numeric",
				"keyword.other.unit",
				"variable.other.import",
				"entity.name.type.package",
				"storage.modifier.package.java",
				"storage.modifier.import.java",
				"keyword.operator.quantifier.regexp",
			],
			"settings": {
				"foreground": "#96be78"
			}
		},
		{
			"scope": [
				"keyword.operator",
				"storage.type.function.arrow.java",
				"keyword.control.ternary.java"
			],
			"settings": {
				"foreground": "#BBBBBB"
			}
		},
		{
			"scope": [
				"support.type",
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#ff9696"
			}
		},
	]
}
